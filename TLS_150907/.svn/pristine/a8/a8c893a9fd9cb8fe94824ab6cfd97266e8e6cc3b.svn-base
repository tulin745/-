#import <Foundation/Foundation.h>

@protocol ViewInteractionInterface <NSObject>
@optional

/*
 *数据交互开启
 */
- (void)viewBeginDataInteraction;

/*
 *数据交互结束
 */
- (void)viewEndDataInteraction;

/*
 *刷新当前那界面
 */
- (void)viewRefreshCurrentInterface;
@end
